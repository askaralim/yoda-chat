    "@qdrant/js-client-rest": "^1.15.1",
    "axios": "^1.6.2",
    "cheerio": "^1.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "langchain": "^1.0.3",
    "mysql2": "^3.15.3",
    "openai": "^6.7.0",
    "qdrant-client": "^0.0.1",
    "redis": "^5.9.0",
    "xml2js": "^0.6.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
